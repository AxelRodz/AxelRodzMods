package com.Axel.ExtraStuff.Tools;

import com.Axel.ExtraStuff.CreativeTab.CreativeTabExtrastuff;

import net.minecraft.item.ItemAxe;

public class AmethystAxe extends ItemAxe {

	public AmethystAxe(ToolMaterial p_i45327_1_) {
		super(p_i45327_1_);
		this.setCreativeTab(CreativeTabExtrastuff.Tab);
		this.setUnlocalizedName("AmethystAxe");
		this.setTextureName("extrastuff:AmethystAxe");
	}
	

}
