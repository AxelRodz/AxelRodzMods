package com.Axel.ExtraStuff.Tools;

import com.Axel.ExtraStuff.CreativeTab.CreativeTabExtrastuff;

import net.minecraft.item.ItemPickaxe;

public class AmethystPickaxe extends ItemPickaxe{

	public AmethystPickaxe(ToolMaterial p_i45347_1_) {
		super(p_i45347_1_);
		this.setCreativeTab(CreativeTabExtrastuff.Tab);
		this.setUnlocalizedName("AmethystPickaxe");
		this.setTextureName("extrastuff:AmethystPickaxe");
	}

}
